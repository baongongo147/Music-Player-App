  <property name="styleSheet">
 <string notr="true">
  /* --- 1. GLOBAL --- */
  QMainWindow {
      background-color: #121212; 
      color: #B3B3B3; 
      font-family: &quot;Segoe UI&quot;, sans-serif;
  }

  /* --- 2. SIDEBAR --- */
  #sidebarFrame {
      background-color: #000000;
      border-right: 2px solid #1DB954; 
  }

  /* --- 3. PLAYER BAR --- */
  #playerBarFrame {
      background-color: #181818;
      border-top: 2px solid #1DB954; 
  }

  /* --- 4. HEADERS (QUEUE, UP NEXT, LIBRARY) --- */
  #labelBackQueue, #labelNextQueue, #labelAllSongs {
      color: #1DB954;            /* Màu xanh lá */
      font-size: 22px;           /* Kích thước bằng Logo MUSIC PLAYER */
      font-weight: 900;          /* Độ đậm nhất (Black/Heavy) */
      text-transform: uppercase; /* Chữ in hoa */
      letter-spacing: 1px;       /* Khoảng cách chữ */
      
      /* Căn chỉnh khoảng cách */
      margin-bottom: 8px;        /* Cách danh sách bên dưới ra một chút cho thoáng */
      margin-left: 0px;          /* Bỏ thụt đầu dòng để thẳng tắp với cạnh trái của khung List */
      padding: 0px;
  }

  /* Riêng header nhỏ "Quick Search" ở Sidebar thì giữ nguyên hoặc chỉnh nhỏ hơn tùy ý */
  #labelLibraryHeader {
      color: #1DB954;
      font-size: 13px;
      font-weight: 800;
      text-transform: uppercase;
      letter-spacing: 1px;
      margin-bottom: 5px;
      margin-left: 5px;
  }

  /* --- 5. LIST WIDGETS (CARD STYLE) --- */
  QListWidget {
      background-color: #1A1A1A; 
      border: 1px solid #282828; 
      border-radius: 8px; 
      outline: none;
      padding: 5px;
      font-family: "Segoe UI", sans-serif;
  }
  
  #listAllSongs::item, #listPlayback::item, #listPlayNext::item, #listHistory::item, #listSearchResult::item {
      min-height: 36px;   
      max-height: 36px;
      padding-left: 5px;
      border-radius: 6px;
      margin-bottom: 2px;
  }

  QListWidget::item {
      color: #B3B3B3;
  }

  QListWidget::item:hover {
      background-color: rgba(255, 255, 255, 0.08); 
  }

  /* KHI ITEM ĐƯỢC CHỌN (SELECTED) */
  QListWidget::item:selected {
      background-color: #1DB954; 
  }

  /* --- LABEL TÊN BÀI HÁT (QUAN TRỌNG) --- */
  /* Mặc định là TRẮNG */
  #lblCardTitle {       
      font-weight: 700;        
      font-size: 13px;
      background-color: transparent;
  } 

  /* Khi Item cha được chọn -> Label con đổi màu XANH */
  QListWidget::item:selected #lblCardTitle { 
      color: #FFFFFF;           
      font-weight: 800; 
  }

  /* --- 6. INPUTS --- */
  QLineEdit {
      background-color: #2A2A2A;
      color: #FFFFFF;
      border: 1px solid #2A2A2A;
      border-radius: 15px;
      padding: 6px 12px;
      selection-background-color: #1DB954;
  }
  QLineEdit:focus {
      background-color: #333333;
      border: 1px solid #1DB954;
  }
  QComboBox {
      background-color: #2A2A2A;
      color: #FFFFFF;
      border: 1px solid #333;
      border-radius: 6px;
      padding: 8px 12px;
  }
  QComboBox::drop-down { border: none; width: 30px; }
  QComboBox QAbstractItemView {
      background-color: #2A2A2A;
      color: #FFFFFF;
      selection-background-color: #1DB954;
  }

  /* --- 7. BUTTONS --- */
  QPushButton { background-color: transparent; border: none; color: #B3B3B3; }
  #btnNavHome, #btnNavPlaylist, #btnNavPersonal {
      text-align: left; padding: 8px 15px; font-weight: 700; border-radius: 4px;
  }
  #btnNavHome:hover, #btnNavPlaylist:hover, #btnNavPersonal:hover {
      color: #FFFFFF; background-color: #282828;
  }
  #btnAddAlbum, #btnAddNext, #btnDelete, #btnSmart, #btnSearch {
      background-color: #2A2A2A; border-radius: 12px;
  }
  #btnAddAlbum:hover, #btnAddNext:hover, #btnDelete:hover, #btnSmart:hover, #btnSearch:hover {
      background-color: #1DB954; color: white;
  }

  /* --- 8. TABS --- */
  QTabWidget::pane { border: 1px solid #282828; background-color: #181818; border-radius: 6px; }
  QTabBar::tab {
      background: #121212; color: #B3B3B3; min-width: 70px; padding: 8px 12px;
      border-top-left-radius: 4px; border-top-right-radius: 4px; margin-right: 2px;
  }
  QTabBar::tab:selected {
      background: #282828; color: #FFFFFF; border-bottom: 3px solid #1DB954; 
  }

  /* --- 9. SLIDERS --- */
  QSlider::groove:horizontal { border: none; height: 4px; background: #404040; border-radius: 2px; }
  QSlider::sub-page:horizontal { background: #1DB954; border-radius: 2px; }
  QSlider::handle:horizontal {
      background: #FFFFFF; width: 10px; height: 10px; margin: -3px 0; border-radius: 5px;
  }
  QSlider::handle:horizontal:hover { transform: scale(1.2); }

  /* --- 10. SCROLL BARS (THANH CUỘN) --- */
  
  /* Thanh dọc */
  QScrollBar:vertical {
      border: none;
      background-color: #121212; /* Nền tối trùng màu App */
      width: 10px;               /* Độ rộng vừa phải */
      margin: 0px;
  }
  /* Cục kéo (Handle) dọc */
  QScrollBar::handle:vertical {
      background-color: #404040; /* Màu xám nổi hơn nền */
      min-height: 30px;
      border-radius: 5px;        /* Bo tròn */
  }
  QScrollBar::handle:vertical:hover {
      background-color: #555555; /* Sáng hơn khi di chuột vào */
  }
  /* Ẩn mũi tên lên xuống */
  QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
      height: 0px;
  }
  QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
      background: none;
  }

  /* Thanh ngang (Cho List Library nếu tên quá dài) */
  QScrollBar:horizontal {
      border: none;
      background-color: #121212;
      height: 10px;
      margin: 0px;
  }
  QScrollBar::handle:horizontal {
      background-color: #404040;
      min-width: 30px;
      border-radius: 5px;
  }
  QScrollBar::handle:horizontal:hover { background-color: #555555; }
  QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal { width: 0px; }

  /* --- 11. OTHERS --- */
  #labelCurrent, #labelDuration { color: #FFFFFF; font-weight: 600; }

  /* --- 12. MESSAGE BOX VÀ DIALOGS --- */
  QMessageBox {
      background-color: #202020;
      border: 2px solid #1DB954; /* Viền xanh lá */
      border-radius: 10px;
  }
  
  /* Chữ trong hộp thoại */
  QMessageBox QLabel {
      color: #FFFFFF;
      font-size: 15px;
      font-weight: 500;
      font-family: "Segoe UI";
      margin-bottom: 10px;
  }

  /* NÚT BẤM TRONG HỘP THOẠI (Style Pill-Shape giống Control) */
  QMessageBox QPushButton {
      background-color: transparent;    /* Nền trong suốt */
      border: 1px solid #555;           /* Viền xám mỏng */
      border-radius: 18px;              /* Bo tròn 18px (Hình viên thuốc) */
      color: #B3B3B3;                   /* Chữ xám nhạt */
      padding: 6px 25px;                /* Padding rộng để dễ bấm */
      font-size: 13px;
      font-weight: 700;
      min-width: 80px;                  /* Chiều rộng tối thiểu */
  }

  /* Hover (Lia chuột vào) */
  QMessageBox QPushButton:hover {
      background-color: rgba(255, 255, 255, 0.1); /* Nền sáng mờ */
      color: #FFFFFF;                             /* Chữ trắng sáng */
      border: 1px solid #1DB954;                  /* Viền chuyển xanh */
  }

  /* Pressed (Nhấn xuống) */
  QMessageBox QPushButton:pressed {
      background-color: rgba(29, 185, 84, 0.2);   /* Nền xanh nhẹ */
  }
 </string>
